<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loginRootPane" prefHeight="400.0" prefWidth="990.0" xmlns="http://javafx.com/javafx/8.0.241" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <TextField fx:id="txtUserName" layoutX="379.0" layoutY="115.0" promptText="UserName">
         <font>
            <Font size="18.0" />
         </font>
      </TextField>
      <Button layoutX="379.0" layoutY="275.0" mnemonicParsing="false" onAction="#Login" text="Login">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <PasswordField fx:id="txtPassword" layoutX="379.0" layoutY="185.0" promptText="Password">
         <font>
            <Font size="18.0" />
         </font>
      </PasswordField>
      <Label fx:id="lblStatus" alignment="CENTER" contentDisplay="CENTER" layoutX="2.0" layoutY="48.0" prefHeight="21.0" prefWidth="990.0" text="Status" textFill="#483fe4">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Button layoutX="524.0" layoutY="275.0" mnemonicParsing="false" onAction="#signup" prefHeight="33.0" prefWidth="91.0" text="SignUp">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
